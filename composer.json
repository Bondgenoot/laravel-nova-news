{
    "name": "mennotempelaar/nova-news-resource-tool",
    "description": "A resource tool for Laravel Nova which adds content management features for managing news posts.",
    "license": "MIT",
    "version": "0.0.0",
    "keywords": [
        "laraval",
        "nova",
        "package",
        "resource",
        "tool",
        "news",
        "cms"
    ],
    "authors": [
        {
            "name": "Menno Tempelaar",
            "email": "men.tempelaar@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "MennoTempelaar\\NovaNewsTool\\": "src",
            "MennoTempelaar\\NovaNewsTool\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MennoTempelaar\\NovaNewsTool\\NewsServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MennoTempelaar\\NovaNewsTool\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
